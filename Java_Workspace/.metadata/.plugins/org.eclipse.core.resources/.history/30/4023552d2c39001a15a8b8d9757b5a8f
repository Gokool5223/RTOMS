package com.app.pojos;

import javax.persistence.*;

public class QuestionOptions 
{
	@Id
	@GeneratedValue
	@Column(name = "ques_op_id")
	private int quesOpId;

	@Column(name = "ans_description")
	private String ansDescription;

	@Column(name = "ques_id")
	private int questionId;
	
	public QuestionOptions() {
	
	}

	public int getQuesOpId() {
		return quesOpId;
	}

	public void setQuesOpId(int quesOpId) {
		this.quesOpId = quesOpId;
	}

	public String getAnsDescription() {
		return ansDescription;
	}

	public void setAnsDescription(String ansDescription) {
		this.ansDescription = ansDescription;
	}

	public int getQuestionId() {
		return questionId;
	}

	public void setQuestionId(int questionId) {
		this.questionId = questionId;
	}

	@Override
	public String toString() {
		return "QuestionOptions [quesOpId=" + quesOpId + ", ansDescription=" + ansDescription + ", questionId="
				+ questionId + "]";
	}
	
	
	


}
